# Firebase configuration - copy to .env and fill with your values
# NEVER commit .env to version control

# Firebase Web API Key (safe to expose client-side when restricted by Security Rules)
VITE_FIREBASE_API_KEY=your_api_key_here

# Firebase project identifiers
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Firebase Realtime Database URL (if using RTDB for fitness tracker)
# Must use HTTPS only
VITE_FIREBASE_DATABASE_URL=https://your_project_id-default-rtdb.firebaseio.com

# Admin password for fitness tracker (client-side only - never use for sensitive ops)
# If unset, admin features are disabled. Exposed in client bundle - use strong password.
VITE_ADMIN_PASSWORD=
